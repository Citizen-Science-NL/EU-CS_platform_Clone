fileignoreconfig:
- filename: .github/workflows/deploy.yml
  checksum: 748c32b09e0db86494c8fa3357fb65b5e5c1863ec59f7da0b3911eff9f90c7a6

- filename: .github/workflows/ci.yml
  checksum: c09a9ed58a7db08e00e0594cd28cacbb1f6156d11ec6c69f63449ac280236a9b
- filename: '*.yml'
  checksum: 69c748939ff97fb6209f0286a053fcc5470ae8475cc8de06ba7d30db1e2275e7
  ignore_detectors: [filename] 

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium


allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}   