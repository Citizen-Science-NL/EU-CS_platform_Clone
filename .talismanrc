fileignoreconfig:
- filename: .github/workflows/deploy.yml
  checksum: 510f5859446f117b0878639433997697d07f1fa93bf517fe4122500ab3efdb93

- filename: .github/workflows/ci.yml
  checksum: a0a9e12ca6b7b6b562196dbe9b9e0109954e3a56bb1aeef7d7b367b37bbda934
- filename: '*.yml'
  checksum: 69c748939ff97fb6209f0286a053fcc5470ae8475cc8de06ba7d30db1e2275e7
  ignore_detectors: [filename] 


scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium

allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}   